package com.himline.chapter3;



public class Array1 {

	public static void main(String[] args) {
		int a[] =new int[5];
		a[0] = 9; 
		a[1] = 8;
		a[2] = 7;
		a[3] = 6;
        a[4] = 5;
        
        
        {
        for(int i=0;i<a.length;i++){
         System.out.println(a[i]);
        }
        }
	}

}
